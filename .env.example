# StudyStreak Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# Suppress build warnings  
GENERATE_SOURCEMAP=false
ESLINT_NO_DEV_ERRORS=true
TSC_COMPILE_ON_ERROR=true
DISABLE_ESLINT_PLUGIN=true
SKIP_PREFLIGHT_CHECK=true

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here

# Payment Gateway (Razorpay)
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_here

# Application Secret Key
REACT_APP_SECRET_KEY=your_secret_key_here

# Google Calendar Integration
REACT_APP_GOOGLE_CALENDER_CLIENT_ID=your_google_calendar_client_id_here
REACT_APP_GOOGLE_CALENDER_API_KEY=your_google_calendar_api_key_here

# Email Service (if used)
REACT_APP_EMAIL_JS_SERVICE_ID=your_emailjs_service_id_here
REACT_APP_EMAIL_JS_TEMPLATE_ID=your_emailjs_template_id_here

# ⚠️ SECURITY WARNING:
# - NEVER add OpenAI API keys to this file
# - All REACT_APP_* variables are PUBLIC in the browser
# - Use Firebase Functions for secure API calls
# - OpenAI integration is handled by backend Firebase Functions